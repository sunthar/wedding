                <div class="item">
                  <div class="row">
                    <div class="col-md-4">
                      <div class="section">
                        <div class="img-circle big_circle_img">
                          <%= image_tag("wedding_party_prasanna.png", :alt => "Prasanna Vasudevan") %>
